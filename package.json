{
  "name": "appBuildCode",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/http-client": "^4.0.2",
    "@sap-cloud-sdk/util": "^4.0.2",
    "@sap/cds": "^8.9.4",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/textbundle": "^5.4.0",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.7.0",
    "express": "^4",
    "hdb": "^0.19.12",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1",
    "@eslint/js": "^9.27.0",
    "@sap/cds-dk": "^8",
    "@sap/dev-cap-tools": "^1.31.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "typescript": "^5",
    "typescript-eslint": "^8.32.1"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-orders-management": "cds watch --open orders-management/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch": "cds-tsx w"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      }
    }
  },
  "lcap": true,
  "sapux": [
    "app/orders-management"
  ],
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  }
}
